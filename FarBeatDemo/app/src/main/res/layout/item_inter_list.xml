<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/inter_list_root"
    style="@style/MaterialDesign"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:padding="8dp">

    <TextView
        android:id="@+id/inter_list_title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:ellipsize="end"
        android:maxLines="1"
        android:text="@string/def_text"
        android:textColor="@color/gray9"
        android:textSize="14sp" />

    <include layout="@layout/vertical_line0" />

    <ImageView
        android:id="@+id/inter_list_result"
        android:layout_width="40dp"
        android:layout_height="19dp"
        android:src="@mipmap/henggang" />
</LinearLayout>